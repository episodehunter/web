{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2015",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
