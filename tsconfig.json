{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2015",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "plugins": [
      {
        "name": "ts-graphql-plugin",
        "schema": {
          "http": {
            "url": "http://devapi.episodehunter.tv/graphql"
          }
        },
        "tag": ""
      }
    ]
  },
  "exclude": ["node_modules"]
}
